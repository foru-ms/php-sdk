{
  "name": "foru-ms/sdk",
  "version": "0.0.34",
  "description": "ForuMs PHP Library",
  "keywords": [
    "foru-ms",
    "api",
    "sdk"
  ],
  "license": [],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "friendsofphp/php-cs-fixer": "3.5.0",
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "ForuMs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ForuMs\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "build": [
      "@php -l src",
      "@php -l tests"
    ],
    "test": "phpunit",
    "analyze": "phpstan analyze src tests --memory-limit=1G"
  }
}